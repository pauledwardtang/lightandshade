
class GameObject
{
   float x, y; // X-coordinate, y-coordinate
   GameObject (float xpos, float ypos)
   {
     x = xpos;
     y = ypos;
   }
   
   float getX()
   {
     return x;
   }
   float getY()
   {
     return y;
   }

}

